{
	"name": "cusodede/yii2-queue-mailer",
	"description": "Queue mailer decorator for Yii2 framework",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"async",
		"mailer",
		"queue"
	],
	"homepage": "https://packagist.org/packages/cusodede/yii2-queue-mailer",
	"license": "MIT",
	"authors": [
		{
			"name": "Alexey Ermachenok",
			"email": "php-masters@yandex.ru"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.0",
		"yiisoft/yii2": "~2",
		"yiisoft/yii2-queue": "~2"

	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5",
		"yiisoft/yii2-swiftmailer": "~2",
		"yiisoft/yii2-symfonymailer": "dev-deffered_transport",
		"pozitronik/yii2-helpers": "^1.0"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/pozitronik/yii2-symfonymailer"
		},
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"autoload": {
		"psr-4": {
			"cusodede\\QueueMailer\\": "./src"
		}
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	}
}
